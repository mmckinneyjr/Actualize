{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data: function () {\n    return {\n      message: \"Welcome to Vue.js!\",\n      products: [],\n      newProductsParams: {}\n    };\n  },\n  created: function () {\n    this.indexProducts();\n  },\n  methods: {\n    indexProducts: function () {\n      axios.get(\"http://localhost:3000/products.json\").then(response => {\n        console.log(\"All Products:\", this.products);\n        this.products = response.data;\n      });\n    },\n    createRecipe: function () {\n      axios.post(\"http://localhost:3000/recipes.json\", this.newRecipeParams).then(response => {\n        console.log(\"Success!\", response.data);\n        this.recipes.push(response.data);\n      }).catch(error => console.log(error.response));\n    }\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,YAAY;IAChB,OAAO;MACLC,OAAO,EAAE,oBADJ;MAELC,QAAQ,EAAE,EAFL;MAGLC,iBAAiB,EAAE;IAHd,CAAP;EAKD,CAPY;EAQbC,OAAO,EAAE,YAAY;IACnB,KAAKC,aAAL;EACD,CAVY;EAWbC,OAAO,EAAE;IACPD,aAAa,EAAE,YAAY;MACzBN,KAAK,CAACQ,GAAN,CAAU,qCAAV,EAAiDC,IAAjD,CAAuDC,QAAD,IAAc;QAClEC,OAAO,CAACC,GAAR,CAAY,eAAZ,EAA6B,KAAKT,QAAlC;QACA,KAAKA,QAAL,GAAgBO,QAAQ,CAACT,IAAzB;MACD,CAHD;IAID,CANM;IAOPY,YAAY,EAAE,YAAY;MACxBb,KAAI,CACDc,IADH,CACQ,oCADR,EAC8C,KAAKC,eADnD,EAEGN,IAFH,CAESC,QAAD,IAAc;QAClBC,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBF,QAAQ,CAACT,IAAjC;QACA,KAAKe,OAAL,CAAaC,IAAb,CAAkBP,QAAQ,CAACT,IAA3B;MACD,CALH,EAMGiB,KANH,CAMUC,KAAD,IAAWR,OAAO,CAACC,GAAR,CAAYO,KAAK,CAACT,QAAlB,CANpB;IAOD;EAfM;AAXI,CAAf","names":["axios","data","message","products","newProductsParams","created","indexProducts","methods","get","then","response","console","log","createRecipe","post","newRecipeParams","recipes","push","catch","error"],"sourceRoot":"","sources":["/Users/codingbootcamp/Documents/Actualize/week05-practice/simple-mini-capstone-api/simple-mini-capstone-frontend/src/views/HomeView.vue"],"sourcesContent":["<script>\nimport axios from \"axios\";\n\nexport default {\n  data: function () {\n    return {\n      message: \"Welcome to Vue.js!\",\n      products: [],\n      newProductsParams: {},\n    };\n  },\n  created: function () {\n    this.indexProducts();\n  },\n  methods: {\n    indexProducts: function () {\n      axios.get(\"http://localhost:3000/products.json\").then((response) => {\n        console.log(\"All Products:\", this.products);\n        this.products = response.data;\n      });\n    },\n    createRecipe: function () {\n      axios\n        .post(\"http://localhost:3000/recipes.json\", this.newRecipeParams)\n        .then((response) => {\n          console.log(\"Success!\", response.data);\n          this.recipes.push(response.data);\n        })\n        .catch((error) => console.log(error.response));\n    },\n  },\n};\n</script>\n\n<template>\n  <div class=\"home\">\n    <h1>{{ message }}</h1>\n    <h2>New Product</h2>\n    <div>\n      Name:\n      <input type=\"text\" v-model=\"newProductsParams.name\" />\n      <br />\n      Description:\n      <input type=\"text\" v-model=\"newProductsParams.description\" />\n      <br />\n      image_url:\n      <input type=\"text\" v-model=\"newProductsParams.image_url\" />\n      <br />\n      price:\n      <input type=\"text\" v-model=\"newProductsParams.price\" />\n    </div>\n    <br />\n    <button v-on:click=\"createNewProduct()\">Create Product</button>\n\n    <div class=\"container\">\n      <div v-for=\"product in products\" v-bind:key=\"product.id\" style=\"padding: 50px 0px 0px 0px\">\n        <img class=\"images\" :src=\"product.image_url\" alt=\"prod\" />\n        <div class=\"name\">{{ product.name }}</div>\n        <div class=\"price\">{{ product.description }}</div>\n        <div class=\"price\">${{ product.price }}</div>\n        <br />\n      </div>\n    </div>\n  </div>\n</template>\n\n<style>\n.name {\n  size: 24px;\n  font-weight: 600;\n  text-align: left;\n}\n.price {\n  size: 18px;\n  text-align: left;\n}\n\n.images {\n  height: 200px;\n}\n.container {\n  display: inline-block;\n  text-align: left;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}